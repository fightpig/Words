                "word_difference_info": [
                    {
                        "common_mean": "要求；请求",
                        "word_list": [
                            "beg",
                            " implore",
                            " request",
                            " pray",
                            " require",
                            " claim",
                            " entreat",
                            " ask",
                            " demand"
                        ],
                        "word_mean_info__list": [
                            {
                                "difference": "指恳切地或再三地请求或要求，常含低三下四意味，也多用于应酬场合。",
                                "word": "beg"
                            },
                            {
                                "difference": "书面用词，着重指迫切、焦急或痛苦地恳求或哀求，常含较强的感情色彩。",
                                "word": "implore"
                            },
                            {
                                "difference": "正式用词，指非常正式，有礼貌的请求或恳求，多含担心因种种原因对方不能答应的意味。",
                                "word": "request"
                            },
                            {
                                "difference": "语气庄重，指热情、诚恳和敬祈的要求，现不很常用。",
                                "word": "pray"
                            },
                            {
                                "difference": "强调根据事业、需要或纪律、法律等而提出的要求。",
                                "word": "require"
                            },
                            {
                                "difference": "指有权或宣称有权得到而公开提出的要求。",
                                "word": "claim"
                            },
                            {
                                "difference": "泛指一般“恳求或哀求”，含企图说服对方或用热烈的请求软化反对意见的意味。",
                                "word": "entreat"
                            },
                            {
                                "difference": "最普通用词，指向对方提出要求或请求，长、晚辈，上下级之间都可使用。",
                                "word": "ask"
                            },
                            {
                                "difference": "一般指理直气壮地提出强烈要求，或坚持不让对方拒绝的要求。",
                                "word": "demand"
                            }
                        ]
                    },
                    {
                        "common_mean": "请求；恳求",
                        "word_list": [
