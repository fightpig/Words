            "book": {
                "yingyinNormal": [
                    {
                        "picUrl": "http://pic01.sogoucdn.com/s3/a/201051/a58ed194f1b80b4b3a378a48a1ea9e4b_23523.jpg",
                        "audioUrl": "http://pic03.sogoucdn.com/s3/a/201051/ff80bf70bf355ea8d4ab9cf38811234f_32719.mp3",
                        "isLavafox": true,
                        "en": "But, <em>Mum</em>, I don't want to lose you again.",
                        "zh": "但是妈妈，我不想再一次失去你。",
                        "videoNameZh": "《小飞侠：幻梦启航》",
                        "videoNameEn": "《Pan》",
