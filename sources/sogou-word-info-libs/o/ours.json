            "book": {
                "yingyinNormal": [
                    {
                        "picUrl": "http://pic04.sogoucdn.com/s3/a/201051/aa7a748a86497e02e2b0a5098f3017b8_12619.jpg",
                        "audioUrl": "http://pic03.sogoucdn.com/s3/a/201051/56f95c188c0bb642b8c96a4ea88304d1_49903.mp3",
                        "isLavafox": true,
                        "en": "You must've eaten one of <em>ours</em>.",
                        "zh": "你肯定没尝过我们的松饼。",
                        "videoNameZh": "《女孩成长记 第一季》",
                        "videoNameEn": "《Girl Meets World Season 1》",
